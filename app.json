{
  "name": "Mailgun Slack webhook",
  "description": "Simple mailgun slack webhook proxy",
  "repository": "https://github.com/amxsv/heroku-slack-mailgun",
  "keywords": ["node", "express", "static", "slack", "mailgun"],
   "env": {
    "MAILGUNAPIKEY": {
      "description": "Mailgun API key",
      "required": "true"
    },
    "SLACKWEBHOOK": {
      "description": "Slack Incomming Webhook URL",
      "required": "true"
    },
    "SLACKCHANNEL": {
      "description": "Slack Channel name",
      "required": "true"
    }
  }
}
